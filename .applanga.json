{
  "app": {
    "base_language": "en",
    "push": {
      "source": [
        {
          "language": "en",
          "file_format": "react_simple_json",
          "path": "./react_json_sample/en/secondsourcefile.json",
          "tag": "SecondFile"
        },
        {
          "language": "en",
          "file_format": "react_simple_json",
          "path": "./react_json_sample/en/translations.json",
          "tag": "FirstFile"
        }
      ]
    },
    "pull": {
      "target": [
        {
          "exclude_languages": [
            "en"
          ],
          "file_format": "react_simple_json",
          "path": "./react_json_sample/<language>/<language>_FirstFile.json",
          "tag": "SecondFile"
        },
        {
          "exclude_languages": [
            "en"
          ],
          "file_format": "react_simple_json",
          "path": "./react_json_sample/<language>/<language>_SecondFile.json",
          "tag": "FirstFile"
        }
      ]
    }
  }
}