{
  "app": {
    "base_language": "en",
    "push": {
      "source": [
        {
          "language": "en",
          "file_format": "nested_json",
          "path": "./react_json_sample/en/secondsourcefile.json",
          "tag": "SecondSource"
        },
        {
          "language": "en",
          "file_format": "nested_json",
          "path": "./react_json_sample/en/translations.json",
          "tag": "Source"
        }
      ]
    },
    "pull": {
      "target": [
        {
          "exclude_languages": [
            "en"
          ],
          "file_format": "nested_json",
          "path": "./react_json_sample/<language>/<language>secondsource.json",
          "tag": "SecondSource"
        },
        {
          "exclude_languages": [
            "en"
          ],
          "file_format": "nested_json",
          "path": "./react_json_sample/<language>/<language>source.json",
          "tag": "Source"
        }
      ]
    }
  }
}